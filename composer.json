{
    "name": "keboola/ex-generic-mock-server",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/lts": "^4@dev",
        "symfony/yaml": "^4.0",
        "symfony/monolog-bundle": "^3.1",
        "squizlabs/php_codesniffer": "^3.2",
        "phpunit/phpunit": "^6.5",
        "symfony/framework-bundle": "^4.0",
        "symfony/browser-kit": "^4.0",
        "symfony/apache-pack": "^1.0"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "id": "01C2KAY4YG32Z3RS35RV0K3578",
            "allow-contrib": false
        }
    }
}
